@model IEnumerable<P02_WebApr2023_Assg1_Team6.Models.DeliveryFailure>

@if (Model.ToList().Count > 0)
{
	<div class="table-responsive">
		<table id="viewStaff" class="table table-striped table-bordered">
			<thead class="table-dark">
				<tr>
					<th>Report ID</th>
					<th>Parcel ID</th>
					<th>Delivery Man ID</th>
					<th>Failure Type</th>
					<th>Description</th>
					<th>StationMgrID</th>
					<th>FollowUpAction</th>
					<th>Date Created</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var item in Model)
				{
					<tr>
						<td>@item.ReportID</td>
						<td>@item.ParcelID</td>
						<td>@item.DeliveryManID</td>
						<td>@item.FailureType</td>
						<td>@item.Description</td>
						<td>@item.StationMgrID</td>
						<td>@item.FollowUpAction</td>
						<td>@item.DateCreated</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
}
else
{
	<span style="color:red">No record found!</span>
}
