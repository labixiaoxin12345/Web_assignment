@model IEnumerable<P02_WebApr2023_Assg1_Team6.Models.DeliveryHistory>

@if (Model.ToList().Count > 0) 
{
	<div class="table-responsive">
		 <table id="viewStaff" class="table table-striped table-bordered">
			 <thead class="table-dark">
				 <tr>
				 <th>Record ID</th>
				 <th>Parcel ID</th>
				 <th>Description</th>
				 </tr>
			</thead>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<td>@item.RecordID.ToString()</td>
							<td>@item.ParcelID.ToString()</td>
							<td>@item.Description</td>
						</tr>
					}
				</tbody>
		</table>
	</div>
}

else
{
	<span style="color:red">No Delivery History record found!</span>
}
